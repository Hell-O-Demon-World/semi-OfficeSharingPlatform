<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.golfzonaca.officesharingplatform.repository.mybatis.CompanyMapper">

    <select id="findOpenDaysById" resultType="java.lang.String">
        select place_opendays
        from place
        where id = #{placeId}
